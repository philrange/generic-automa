<!DOCTYPE html>
<html>
  <head>
    <title>GenericAutoma</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
      
    <% if content_for? :for_head %>
        <%= yield :for_head %>
    <% end %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
